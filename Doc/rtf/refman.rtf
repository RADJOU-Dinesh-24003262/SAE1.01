00048     {\cf18 float} DownSocle1_y    = TopSocle2_y;\par
00049     {\cf18 float} DownSocle2_x    = TopSocle2_x + (3/ratio);\par
00050     {\cf18 float} DownSocle2_y    = TopSocle2_y + (5/ratio);\par
00051     {\cf18 float} Hand_size_x     = body_size/3;\par
00052     {\cf18 float} Hand_size_y     = body_size/2;\par
00053     {\cf18 float} Hand1_x         = TopSocle1_x;\par
00054     {\cf18 float} Hand2_x         = TopSocle2_x;\par
00055     {\cf18 float} Hand_y          = (5/ratio) + Center2 - (5/ratio);\par
00056     {\cf18 float} Shoulder1_x     = Center1 - body_size;\par
00057     {\cf18 float} Shoulder1_y     = (5/ratio) + Center2 - body_size;\par
00058     {\cf18 float} Shoulder2_x     = Center1 + body_size;\par
00059     {\cf18 float} Shoulder2_y     = (5/ratio) + Center2 - (body_size/2);\par
00060     {\cf18 float} LeftBretelle1_x = Center1 - (body_size/2) - (1/ratio);\par
00061     {\cf18 float} RightBretelle1_x= Center1 + (body_size/2) - (1/ratio);\par
00062     {\cf18 float} Bretelle1_y     = Shoulder1_y;\par
00063     {\cf18 float} LeftBretelle2_x = Center1 - (body_size/2) + (1/ratio);\par
00064     {\cf18 float} RightBretelle2_x= Center1 + (body_size/2) + (1/ratio);\par
00065     {\cf18 float} Bretelle2_y     = Shoulder2_y;\par
00066     {\cf18 float} LeftButton_x    = LeftBretelle1_x + ((LeftBretelle2_x-LeftBretelle1_x)/2);\par
00067     {\cf18 float} RightButton_x   = RightBretelle1_x + ((RightBretelle2_x-RightBretelle1_x)/2);\par
00068     {\cf18 float} Button_y        = Bretelle2_y;\par
00069     {\cf18 float} Button_size     = (2/ratio);\par
00070     {\cf18 float} Head_size       = (8/ratio);\par
00071     {\cf18 float} Head_x          = Center1;\par
00072     {\cf18 float} Head_y          = (5/ratio) + Center2 - body_size - (2*(Head_size/3));\par
00073     {\cf18 float} TopHat_size_x   = Head_size+1;\par
00074     {\cf18 float} TopHat_size_y   = 2*(Head_size/5);\par
00075     {\cf18 float} TopHat_x        = Center1;\par
00076     {\cf18 float} TopHat_y        = Head_y - Head_size + (2/ratio);\par
00077     {\cf18 float} DownHat_size_x  = Head_size;\par
00078     {\cf18 float} DownHat_size_y  = TopHat_size_y/3;\par
00079     {\cf18 float} DownHat_x       = Center1;\par
00080     {\cf18 float} DownHat_y       = TopHat_y + TopHat_size_y;\par
00081 \par
00082     {\cf20 //shape formant le sprite}\par
00083     window << nsShape::Rectangle(nsGraphics::Vec2D((Center1-wall_x),(Center2-wall_y)), nsGraphics::Vec2D((Center1+wall_x),(Center2+wall_y)), nsGraphics::KBlack);{\cf20 // square}\par
00084     window << nsShape::Oval(nsGraphics::Vec2D(Hand1_x,Hand_y), nsGraphics::Vec2D(Hand_size_x,Hand_size_y), nsGraphics::KBeige);{\cf20 // pied droit}\par
