00090     window << nsShape::Rectangle(nsGraphics::Vec2D(DownSocle1_x,DownSocle1_y), nsGraphics::Vec2D(DownSocle2_x,DownSocle2_y), nsGraphics::KGray);{\cf20 // bas du socle}\par
00091     window << nsShape::Rectangle(nsGraphics::Vec2D(Shoulder1_x,Shoulder1_y), nsGraphics::Vec2D(Shoulder2_x,Shoulder2_y), nsGraphics::KYellow);{\cf20 // \'E9paule}\par
00092     window << nsShape::Rectangle(nsGraphics::Vec2D(LeftBretelle1_x,Bretelle1_y), nsGraphics::Vec2D(LeftBretelle2_x,Bretelle2_y), nsGraphics::KPurple);{\cf20 // bretelle}\par
00093     window << nsShape::Rectangle(nsGraphics::Vec2D(RightBretelle1_x,Bretelle1_y), nsGraphics::Vec2D(RightBretelle2_x,Bretelle2_y), nsGraphics::KPurple);{\cf20 // bretelle}\par
00094     window << nsShape::Circle(nsGraphics::Vec2D(LeftButton_x, Button_y), Button_size, nsGraphics::KWhite);  {\cf20 //bouton gauche}\par
00095     window << nsShape::Circle(nsGraphics::Vec2D(RightButton_x, Button_y), Button_size, nsGraphics::KWhite);  {\cf20 //bouton droit}\par
00096     window << nsShape::Circle(nsGraphics::Vec2D(Head_x,Head_y), Head_size, nsGraphics::KBeige);  {\cf20 //t\'EAte}\par
